    "format": "prettier --write ."
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "pinia": "^2.1.7",
    "element-plus": "^2.6.3",
    "@element-plus/icons-vue": "^2.3.1",
    "echarts": "^5.5.0",
    "vue-echarts": "^6.7.0",
    "axios": "^1.6.8",
    "phosphor-icons": "^1.4.2",
    "@phosphor-icons/vue": "^2.1.6",
    "dayjs": "^1.11.10",
    "uuid": "^9.0.1",
    "@vueuse/core": "^10.9.0",
    "localforage": "^1.10.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "vite": "^5.2.0",
    "typescript": "^5.2.2",
    "vue-tsc": "^2.0.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "prettier": "^3.2.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "sass": "^1.74.1"
